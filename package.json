{
  "name": "01-nextjs-typeorm-auth",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "db:migrate": "dotenv -e .env.production.local -- npx prisma migrate deploy",
    "prisma:studio": "dotenv -e .env.production.local -- npx prisma studio --browser none",
    "dev:db:prisma:studio": "dotenv -e .env.development.local -- npx prisma studio --browser none",
    "dev:db:seed": "dotenv -e .env.development.local -- npx tsx ./src/database/utils/dev/seed.ts",
    "dev:db:migrate": "dotenv -e .env.development.local -- npx prisma migrate dev",
    "dev:db:reset": "dotenv -e .env.development.local -- npx prisma migrate reset --force --skip-seed",
    "dev:db:clear": "npx tsx ./src/database/utils/dev/reset.ts",
    "test:build": "dotenv -e .env.test.local -- next build",
    "test:start": "dotenv -e .env.test.local -- next start",
    "test": "npm run test:db:push && npm run test:build && npm run test:start",
    "test:db:prisma:studio": "dotenv -e .env.test.local -- npx prisma studio --browser none",
    "test:db:migrate": "dotenv -e .env.test.local -- npx prisma migrate deploy",
    "test:db:push": "dotenv -e .env.test.local -- npx prisma db push --force-reset --accept-data-loss",
    "test:db:reset": "npm run test:db:push && dotenv -e .env.test.local -- npx prisma migrate reset --force",
    "test:db:generate": "dotenv -e .env.test.local -- npx prisma generate",
    "test:db:clear": "dotenv -e .env.test.local -- npx tsx ./src/database/utils/test/clear.ts",
    "test:jest": "dotenv -e .env.test.local -- jest",
    "test:cypress:open": "dotenv -e .env.test.local -- cypress open",
    "test:cypress:e2e": "dotenv -e .env.test.local -- cypress run --e2e"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.1",
    "@base-ui-components/react": "^1.0.0-beta.1",
    "@mantine/core": "^8.3.1",
    "@mantine/dates": "^8.3.1",
    "@mantine/hooks": "^8.3.1",
    "@mantine/notifications": "^8.3.1",
    "@next/env": "^15.4.5",
    "@prisma/client": "^6.10.0",
    "@tanstack/react-table": "^8.21.3",
    "@vercel/blob": "^2.0.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.18",
    "immer": "^10.1.3",
    "jose": "^6.0.11",
    "next": "15.3.3",
    "next-auth": "^5.0.0-beta.28",
    "nodemailer": "^6.10.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "use-immer": "^0.11.0",
    "uuid": "^11.1.0",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/cypress": "^0.1.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cypress": "^14.5.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prisma": "^6.10.0",
    "sass": "^1.89.2",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
